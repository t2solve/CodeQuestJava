class Fibo {
    public static void main(String args[]) {
        int n = 4;
        int result = getFiboResult(n);
        System.out.println(result);
    }

    private static int getFiboResult(int n) {
        int count = 0;
        int sum = 0;
        int a = 0;
        int b = 1;
        while (count < n) {
            int next = a + b;
            a = b;
            b = next;
            if (b % 2 == 0) {
                count++;
                sum = b;
            }
        }
        return sum;
    }
}
